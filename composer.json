{    "name": "tuliofergulha/poc-buscardor-cursos",    "description": "Projeto para buscar cursos",    "type": "library",    "authors": [        {            "name": "Túlio Fergulha",            "email": "tfergulha@gmail.com"        }    ],    "require": {        "symfony/dom-crawler": "^5.3",        "guzzlehttp/guzzle": "^7.3",        "symfony/css-selector": "^5.3"    },    "autoload": {        "files": ["./functions.php"],        "psr-4": {            "BuscadorCursos\\": "src/"        }    },    "require-dev": {        "phpunit/phpunit": "^9.5",        "squizlabs/php_codesniffer": "^3.6",        "phan/phan": "^5.2"    },    "scripts": {        "test": "phpunit tests\\TestBuscadorDeCursos.php",        "cs": "phpcs --standard=PSR12 src\\",        "phan": "phan --allow-polyfill-parser",        "check": [            "@phan",            "@ps",            "@test"        ],        "post-update-cmd": [            "@test"        ]    },    "scripts-descriptions": {        "check": "Roda as verificações do código. PHAN, PHPCS e PHPUNIT."    }}